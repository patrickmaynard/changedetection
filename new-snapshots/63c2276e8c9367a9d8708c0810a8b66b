    var searchID = ["International-Travel-Country-Information-Pages"];
    
    $( ".searchCSI #"+ searchID).autocomplete({
        source: availableTags,
        autoFocus: true,
        messages: 
        { 
            noResults: '', 
            results: function() {} 
        }, 
        select: function (event, ui) {
            window.location = ui.item.url+".html";
            $(this).val('');
            return false;
        },
        response: function (event, ui) {
            if (!ui.content.length) {
                alert("No results");
                $("#"+ searchID).val('No results');
            }
        },
        focus: function (event, ui) {
            currentUrl = ui.item.url+".html";
        }
    });
    $(".searchCSI .searchbtn").click(function () {
        btnGSearchClick();
    }); 
    
    function go(url) {
        window.location = url;
    }
     function btnGSearchClick() {
        if (currentUrl !== "") go(currentUrl);
    }   
});
</script>
